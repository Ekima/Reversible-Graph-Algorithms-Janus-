procedure removeedge(int G[], int node1, int node2, int removededge)
    removededge += G[node1-1][node2-1]
    G[node1-1][node2-1] -= removededge
    if node1 != node2 then
    	G[node2-1][node1-1] -= removededge
    fi node1 != node2

procedure main()
    int G[][] = {{0, 2, 4},
                 {2, 0, 3},
                 {4, 3, 0}}
    int removededge
    
    call removeedge(G, 1, 3, removededge)